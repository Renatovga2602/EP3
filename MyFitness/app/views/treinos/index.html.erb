</br><center><h1>Treinos</h1></center>

<table class="table table-bordered table-hover">
  <thead>
    <tr>
      <th>Inferior</th>
      <th>Triceps</th>
      <th>Abslombar</th>
      <th>Dorsais</th>
      <th>Deltoides</th>
      <th>Biceps</th>
      <th>Peitorais</th>
      <th>Series</th>
      <th>Repeticoes</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @treinos.each do |treino| %>
      <tr>
        <td><%= treino.inferior %></td>
        <td><%= treino.triceps %></td>
        <td><%= treino.abslombar %></td>
        <td><%= treino.dorsais %></td>
        <td><%= treino.deltoides %></td>
        <td><%= treino.biceps %></td>
        <td><%= treino.peitorais %></td>
        <td><%= treino.series %></td>
        <td><%= treino.repeticoes %></td>
        <td><%= link_to 'Acessar', treino %></td>
        <td><%= link_to 'Editar', edit_treino_path(treino) %></td>
        <td><%= link_to 'Remover', treino, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
